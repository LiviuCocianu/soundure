{
  "expo": {
    "name": "Soundure",
    "slug": "Soundure",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon/icon.png",
    "scheme": "soundure",
    "splash": {
      "image": "./assets/images/soundure_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/foreground_icon.png",
        "backgroundImage": "./assets/icon/background_icon.png"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "io.github.idoomful.Soundure.android"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Aplica»õia are nevoie de acces la galerie."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "cbe15ea3-3fe9-4136-946f-3431c1beae3b"
      }
    },
    "owner": "idoomful",
    "sdkVersion": "48.0.0"
  },
  "name": "soundure"
}